{
  "name": "fond-of-spryker/configurable-sort",
  "description": "Configurable sort module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=7.2",
    "spryker/search-extension": "^1.0.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "codeception/mockery-module": "^0.2",
    "mikey179/vfsstream": "^1.6",
    "spryker/code-sniffer": "^0.12",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "php-coveralls/php-coveralls": "^2.1",
    "fond-of-codeception/spryker": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  }
}
